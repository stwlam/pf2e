                    "remaster": true,
                    "title": "Starfinder Player Core"
                },
                "rules": [
                    {
                        "key": "AdjustModifier",
                        "predicate": [
                            {
                                "or": [
                                    "penalty:slug:armor-speed-penalty",
                                    "penalty:slug:armor-check-penalty"
                                ]
                            },
                            {
                                "gte": [
                                    "attribute:con:mod",
                                    "armor:strength"
                                ]
                            }
                        ],
                        "selectors": [
                            "speed",
                            "skill-check"
                        ],
                        "suppress": true
                    },
                    {
                        "key": "ActiveEffectLike",
                        "mode": "add",
